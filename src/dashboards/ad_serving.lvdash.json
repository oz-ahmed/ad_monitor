{
  "datasets": [
    {
      "displayName": "real time budget monitoring",
      "name": "fc3c2bf6",
      "queryLines": [
        "SELECT \r\n",
        "    advertiser_id,\r\n",
        "    spend_date,\r\n",
        "    daily_gross_spend,\r\n",
        "    daily_net_spend,\r\n",
        "    current_daily_budget,\r\n",
        "    remaining_gross_budget,\r\n",
        "    remaining_net_budget,\r\n",
        "    gross_budget_utilization_pct,\r\n",
        "    net_budget_utilization_pct,\r\n",
        "    ad_serving_status,\r\n",
        "    CASE \r\n",
        "        WHEN gross_budget_utilization_pct >= 100 THEN 'ðŸ”´ STOPPED'\r\n",
        "        WHEN gross_budget_utilization_pct >= 95 THEN 'ðŸŸ  CRITICAL'\r\n",
        "        WHEN gross_budget_utilization_pct >= 80 THEN 'ðŸŸ¡ WARNING'\r\n",
        "        ELSE 'ðŸŸ¢ ACTIVE'\r\n",
        "    END as status_indicator,\r\n",
        "    calculated_at\r\n",
        "FROM ad_monitor.gold.gold_daily_advertiser_spend_summary\r\n",
        "WHERE spend_date <= CURRENT_DATE()\r\n",
        "ORDER BY gross_budget_utilization_pct DESC;"
      ]
    },
    {
      "displayName": "critical budget alerts",
      "name": "2f294ae2",
      "queryLines": [
        "SELECT \r\n",
        "    advertiser_id,\r\n",
        "    daily_gross_spend,\r\n",
        "    current_daily_budget,\r\n",
        "    remaining_gross_budget,\r\n",
        "    gross_budget_utilization_pct,\r\n",
        "    ad_serving_status,\r\n",
        "    CASE \r\n",
        "        WHEN remaining_gross_budget <= 0 THEN 'BUDGET EXCEEDED - STOP ADS'\r\n",
        "        WHEN remaining_gross_budget <= current_daily_budget * 0.05 THEN 'CRITICAL - 5% REMAINING'\r\n",
        "        WHEN remaining_gross_budget <= current_daily_budget * 0.20 THEN 'WARNING - 20% REMAINING'\r\n",
        "    END as alert_message,\r\n",
        "    calculated_at\r\n",
        "FROM ad_monitor.gold.gold_daily_advertiser_spend_summary\r\n",
        "WHERE spend_date <= CURRENT_DATE()\r\n",
        "    AND (\r\n",
        "        gross_budget_utilization_pct >= 80 \r\n",
        "        OR ad_serving_status IN ('STOPPED', 'CRITICAL', 'WARNING')\r\n",
        "    )\r\n",
        "ORDER BY gross_budget_utilization_pct DESC;"
      ]
    },
    {
      "displayName": "Hourly Spend Utilization",
      "name": "5b6468e6",
      "queryLines": [
        "SELECT \r\n",
        "    advertiser_id,\r\n",
        "    moment as hour_moment,\r\n",
        "    hourly_gross_spend,\r\n",
        "    hourly_net_spend,\r\n",
        "    SUM(hourly_gross_spend) OVER (\r\n",
        "        PARTITION BY advertiser_id \r\n",
        "        ORDER BY moment \r\n",
        "        ROWS UNBOUNDED PRECEDING\r\n",
        "    ) as cumulative_gross_spend,\r\n",
        "    SUM(hourly_net_spend) OVER (\r\n",
        "        PARTITION BY advertiser_id \r\n",
        "        ORDER BY moment \r\n",
        "        ROWS UNBOUNDED PRECEDING\r\n",
        "    ) as cumulative_net_spend,\r\n",
        "    calculated_at\r\n",
        "FROM ad_monitor.silver.silver_hourly_spend_summary\r\n",
        "WHERE DATE(calculated_at) <= CURRENT_DATE()\r\n",
        "    AND advertiser_id IN (\r\n",
        "        SELECT advertiser_id \r\n",
        "        FROM ad_monitor.gold.gold_daily_advertiser_spend_summary \r\n",
        "        WHERE spend_date <= CURRENT_DATE()\r\n",
        "        ORDER BY gross_budget_utilization_pct DESC \r\n",
        "        LIMIT 10\r\n",
        "    )\r\n",
        "ORDER BY advertiser_id, moment;"
      ]
    },
    {
      "displayName": "Budget Change Impact",
      "name": "71a0124a",
      "queryLines": [
        "SELECT \r\n",
        "    advertiser_id,\r\n",
        "    budget_change_time,\r\n",
        "    previous_budget,\r\n",
        "    new_budget,\r\n",
        "    budget_change,\r\n",
        "    budget_change_pct,\r\n",
        "    budget_change_type,\r\n",
        "    CASE \r\n",
        "        WHEN budget_change_type = 'INCREASE' THEN 'ðŸ“ˆ INCREASED'\r\n",
        "        WHEN budget_change_type = 'DECREASE' THEN 'ðŸ“‰ DECREASED'\r\n",
        "        ELSE 'ðŸ†• INITIAL'\r\n",
        "    END as change_indicator,\r\n",
        "    processed_at\r\n",
        "FROM ad_monitor.gold.gold_budget_change_impact\r\n",
        "WHERE DATE(budget_change_time) <= CURRENT_DATE()\r\n",
        "ORDER BY budget_change_time DESC;"
      ]
    }
  ],
